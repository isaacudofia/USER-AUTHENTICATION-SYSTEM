datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User{
    id    String @id @default(uuid())
    username String @unique
    password String //Stores the bcrypt hashed password
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}